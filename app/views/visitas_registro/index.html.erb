<%= render 'layouts/header_normal' %>
<div class="row"></div>
<div class="row">
  <div class="col-lg-12">
    <div class="table-responsive">
      <div class="content-wrapper" style="min-height: 100%">

        <section class="content">
          <div class="table table-bordered menu-principal">
            <div class="content-div">
              <div>
                <h2>FRACCIONAMIENTO</h2>
                <h4>NOMBRE DEL FRACCIONAMIENTO</h4>
                <h4>FECHA</h4>
              </div>
              <div class="div-container-table">
                <table>
                  <th>FECHA</th>
                  <th>HORA</th>
                  <th>CASA</th>
                  <th>NOMBRE</th>
                  <th>ACCESO</th>
                  <th>IDENTIFICACION</th>
                  <% @v_registro.each do |visita|%>
                  <tr>
                    <td><%= visita.fecha %></td>
                    <td><%= visita.hora %></td>
                    <td><%= visita.casa %></td>
                    <td><%= visita.nombre_visitante.to_s + " " + visita.apellidos_visitante.to_s %></td>
                    <td><%= visita.tipo_visita %></td>
                    <td><%= visita.foto_identificacion %></td>
                  </tr>
                      <% end %>
                </table>
              </div>
            </div>
          </div>
        </section>
      </div>
    </div>
  </div>
</div>